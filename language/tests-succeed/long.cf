val debug_s32(n: s32): s32 {
  log(n);
  n
}

val should_recurse(n: s32): bool {
  2 <= n
}
val fib(n: s32): s32 {
  if should_recurse(n) {
    fib(n + -1) + fib(n + -2)
  } else {
    n
  }
}
val bloop(x: bool, y: unit): s32 {
